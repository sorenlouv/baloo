{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "no-console": 0,
    "import/no-commonjs": 0,
    "arrow-body-style": 0
  },
  "plugins": ["prettier"],
  "extends": [
    "recommended/node",
    "recommended/esnext",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ]
}
